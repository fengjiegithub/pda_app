                android:id="@+id/text22"
                android:textStyle="bold"
                android:layout_marginTop="15dp"
                android:textColor="#3E9C42"
                android:layout_marginLeft="10dp"
                android:textSize="15sp"/>
        </android.support.constraint.ConstraintLayout>
